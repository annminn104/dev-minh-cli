{
  "name": "xaca-web-page",
  "private": true,
  "version": "1.0.6",
  "type": "module",
  "engines": {
    "node": "16.20.*",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --ext \"src/**/(*.tsx|*.ts|*.css|*.scss)\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix --ext \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen:component": "hygen components new",
    "gen:container": "hygen containers new",
    "gen:page": "hygen pages new",
    "prepare": "husky install | chmod ug+x .husky/*",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@faker-js/faker": "^8.0.2",
    "@hookform/resolvers": "^3.1.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/addon-controls": "^7.1.1",
    "@storybook/addon-docs": "^7.0.24",
    "@storybook/preset-create-react-app": "^7.0.24",
    "@storybook/preset-scss": "^1.0.3",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "antd": "^5.6.3",
    "auto-changelog": "^2.4.0",
    "hygen": "^6.2.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.45.1",
    "react-modal": "^3.16.1",
    "react-phone-number-input": "^3.3.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0",
    "redux": "^4.2.1",
    "sass": "^1.63.6",
    "socket.io-client": "^4.7.1",
    "vite-plugin-svgr": "^3.2.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.24",
    "@storybook/addon-interactions": "7.0.24",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-links": "7.0.24",
    "@storybook/blocks": "7.0.24",
    "@storybook/builder-vite": "^7.0.24",
    "@storybook/react": "7.0.24",
    "@storybook/react-vite": "7.0.24",
    "@storybook/testing-library": "0.0.14-next.2",
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "axios": "^1.4.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "prettier": "^2.8.8",
    "prop-types": "15.8.1",
    "react-currency-input-field": "^3.6.11",
    "react-loading-skeleton": "^3.3.1",
    "storybook": "7.0.24",
    "storybook-addon-react-router-v6": "^1.0.2",
    "storybook-dark-mode": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
